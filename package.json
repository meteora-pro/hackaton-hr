    "format:check": "nx format:check",
    "update": "ng update @nrwl/workspace",
    "workspace-schematic": "nx workspace-schematic",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "typeorm": "ts-node -P ./apps/api/tsconfig.app.json -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "up:compose": "docker-compose -f ./docker-compose.dev.yml --env-file ./config/api/.development.env up -d",
    "down:compose": "docker-compose -f ./docker-compose.dev.yml --env-file ./config/api/.development.env down",
    "migration": "npm run typeorm -- migration:generate -n change-schema"
